<?xml version="1.0" encoding="UTF-8"?>

<manifest>

    <remote name="vendor"
            fetch="https://gitlab.com/"
            revision="snow" />

    <remote name="toolchain"
            fetch="https://github.com"
            revision="main" />
	
    <remote name="snow"
            fetch="https://github.com"
            revision="octavi"/>

    <remote name="kernel"
            fetch="https://github.com"
            revision="神速"/>

    <remote name="los"
            fetch="https://github.com"
            revision="lineage-19.0"/>
	
   <remote name="los-caf-8250"
            fetch="https://github.com"
            revision="lineage-19.0-caf-sm8250"/>
	
	
   <remove-project name="LineageOS/android_hardware_qcom_audio" />
   <remove-project name="LineageOS/android_hardware_qcom_display" />
            

    <!--Device Trees-->
    <project path="device/xiaomi/monet" name="mikairyuu/android_device_xiaomi_monet-oss" remote="snow" />
    <project path="device/xiaomi/sm7250-common" name="mikairyuu/android_device_xiaomi_sm7250-common" remote="snow" />

    <!--Kernel-->
    <project path="kernel/xiaomi/sm7250" name="mikairyuu/kernel_xiaomi_sm7250" remote="kernel" />

    <project path="hardware/xiaomi" name="LineageOS/android_hardware_xiaomi" remote="los" />
	
    <project path="hardware/qcom-caf/sm8250/audio" name="LineageOS/android_hardware_qcom_audio" remote="los-caf-8250" />
    <project path="hardware/qcom-caf/sm8250/display" name="LineageOS/android_hardware_qcom_display" remote="los-caf-8250" />



    <!-- Vendor folders -->
    <project path="vendor/xiaomi/" name="mikairyuu/android-vendor-xiaomi-sm7250" remote="vendor" />
    
    <project path="prebuilts/clang/host/linux-x86/clang-lolz" name="Jprimero15/lolz-clang" remote="toolchain" clone-depth="1"/>
</manifest>
