<?xml version="1.0" encoding="UTF-8"?>

<manifest>

    <remote name="vendor"
            fetch="https://gitlab.com/"
            revision="Monet" />
	
    <remote name="snow"
            fetch="https://github.com"
            revision="snow"/>

    <remote name="kernel"
            fetch="https://github.com"
            revision="pe12"/>
            

    <!--Device Trees-->
    <project path="device/xiaomi/monet" name="mikairyuu/android_device_xiaomi_monet-oss" remote="snow" />
    

    <!--Kernel-->
    <project path="kernel/xiaomi/sm7250" name="maluueu/android_kernel_xiaomi_sm7250" remote="kernel" />



    <!-- Vendor folders -->
    <project path="vendor/xiaomi/" name="xSylla/android-vendor-xiaomi-sm7250" remote="vendor" />
    
    <project path="device/xiaomi/sm7250-common" name="mikairyuu/android_device_xiaomi_sm7250-common" remote="snow" />
	
    <!-- Fixes -->
	<remove-project name="frameworks_native" />
	<project path="frameworks/native" name="mikairyuu/frameworks_native" remote="snow" />
	
	
</manifest>
